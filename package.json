{
  "name": "push-ios-mongo",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && concurrently \"node dist/index.js\" \"node dist/workers.js\"",
    "worker": "node dist/workers.js",
    "start:api": "node dist/index.js",
    "start:worker": "node dist/workers.js",
    "dev": "tsc && nodemon dist/index.js",
    "dev:worker": "tsc && nodemon dist/workers.js",
    "dev:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose"
  },
  "dependencies": {
    "apn": "^2.2.0",
    "bullmq": "^5.57.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^8.17.1",
    "p-limit": "^7.0.0",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.2.1",
    "@types/supertest": "^6.0.3",
    "jest": "^30.0.5",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "typescript": "^5.0.0"
  }
}
